{
  "expo": {
    "name": "Pok√©mon Count!",
    "slug": "PokemonCount",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/images/133.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/133.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.vutriminh.PokemonCount"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/133.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/133.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "b362d8ed-a6fe-4a01-9688-a98776b28f19"
      }
    }
  }
}
